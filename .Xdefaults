! Low contrast theme, based on Zenburn <http://slinky.imukuppi.org/zenburnpage/>
! Modified from <https://gist.github.com/564084>
URxvt.foreground:       [100]#0c0c0c
URxvt.color0:           #3f3f3f
URxvt.color1:           #705050
URxvt.color2:           #00b45a
URxvt.color3:           #dfcf8f
URxvt.color4:           #506070
URxvt.color5:           #dc8cc3
URxvt.color6:           #8cd0d3
URxvt.color7:           #dcdccc
URxvt.color8:           #709080
URxvt.color9:           #cc5454
URxvt.color10:          #4d9f4d
URxvt.color11:          #f0dfaf
URxvt.color12:          #4186be
URxvt.color13:          #ec93d3
URxvt.color14:          #93e0e3
URxvt.color15:          #ffffff
URxvt.depth:		32
URxvt.background:	#ffffff

! Fake transparency
!URxvt*transparent: 	true
!URxvt*shading:		30

!! URxvt Appearance
URxvt.font: xft:inconsolata:pixelsize=14
URxvt.reverseVideo: True
URxvt.letterSpace: 0
URxvt.lineSpace: 0
URxvt.internalBorder: 48
URxvt.cursorBlink: true
URxvt.cursorUnderline: false
URxvt.saveline: 4092
URxvt.scrollBar: false
URxvt.scrollBar_right: false
URxvt.urgentOnBell: true
!URxvt.depth: 24
URxvt.iso14755: false

! do not scroll with output
URxvt*scrollTtyOutput: false

! scroll in relation to buffer (with mouse scroll or Shift+Page Up)
URxvt*scrollWithBuffer: true

! scroll back to the bottom on keypress
URxvt*scrollTtyKeypress: true

! fix scrolling while in a secondary screen (like in less). otherwise it just scrolls up the terminal I think
!URxvt.secondaryScreen: 1
!URxvt.secondaryScroll: 0

URxvt.perl-ext-common: default,matcher
URxvt.url-launcher: /usr/bin/firefox
URxvt.matcher.button: 1
